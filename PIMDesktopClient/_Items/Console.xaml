<UserControl x:Class="WindowsClient._Data._Items.Console"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WindowsClient._Data._Items"
             mc:Ignorable="d" 
             d:DesignHeight="400" d:DesignWidth="600">
    <Border Background="#FF405089" BorderBrush="White" BorderThickness="1" CornerRadius="5" SnapsToDevicePixels="True">
        <TextBlock x:Name="Viewer" ScrollViewer.VerticalScrollBarVisibility="Auto" Margin="10,10,10,7" Foreground="White" FontFamily="Noto Sans" FontSize="14"/>
    </Border>
</UserControl>
